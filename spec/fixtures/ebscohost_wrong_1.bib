@article{2008-18679-00720070101,
Abstract = {This article discusses the African American lesbian gang, DTO (Dykes Taking Over), as an example of a student-initiated strategy for dealing with homophobic bullying in an urban American school district. A series of alleged incidents of same-sex sexual harassment by gang members on heterosexual students illustrate how lesbian/bisexual threat was used by these women to re-establish a power differential after they experienced bullying based on their sexuality and gender expression. A series of alleged incidents of same-sex sexual harassment by gang members on female heterosexual students illustrate how gay/bisexual threat was used by these women to re-establish a power differential after they experienced bullying based on their sexuality and gender expression. This article considers how these students were reacting, perhaps preemptively and in retaliation, to homophobia in their schools, particularly from their peers, forming gangs and using same sex sexual harassment of other students as a weapon against homophobia and a means by which they could assert themselves in their masculinities, not unlike their male peers who experience same sex bullying and/or harassment and use anti-female sexual harassment to assert their masculinity. Intersections of gender, race, ethnicity, class, and sexuality frame several major questions that arise from these considerations, including: Might their masculinities be uniquely related to their performances of bullying? (How) could homophobic bullying be framed with sexual harassment in both policy and practice? Would this framing benefit or harm students who are bullied? How would/does that change the way we can handle it in schools (i.e., school policies), if at all? Implications for school-based practitioners are discussed with regard to how these students' behavior might be the result of a lack of programs and services available for LGBTQ and same gender loving youth both in and after school. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
Author = {Johnson, Dominique},
ISSN = {1053-8720, 1540-4056},
Journal = {Journal of Gay & Lesbian Social Services: Issues in Practice, Policy & Research},
Keywords = {African Americans, lesbians, gangs, homophobic bullying, urban school district, sexual harassment, Juvenile Gangs, Lesbianism, Sexual Harassment, Urban Environments, Bullying, Adolescent Attitudes, Blacks, High Schools, Homosexuality (Attitudes Toward)},
Number = {3-4},
Pages = {87 - 104},
Title = {Taking over the school: Student gangs as a strategy for dealing with homophobic bullying in an urban public school district},
URL = {djohnson@beamyouth.org},
Volume = {19},
Year = {2007},
}


@article{1995-40427-00119950701,
Abstract = {Parents of 181 pairs of same-sex twins (aged 7–15 yrs) completed the Child Behavior Checklist (CBCL), which was scored on the Child Behavior Profile. Correlations between scores on the CBCL scales for the 99 monozygotic and 82 dizygotic pairs of twins indicate genetic influences that varied according to the specific area of competence and problem behavior. Genetic effects were highest for attention problems, somatic complaints, social problems, withdrawal, and aggressive behavior. Shared environmental influence was detected for participation in activities, quality of social relationships, school performance, anxiety/depression, and delinquent behavior. (PsycINFO Database Record (c) 2019 APA, all rights reserved)},
