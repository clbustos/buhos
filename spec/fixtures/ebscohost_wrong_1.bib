ISSN = {1525-4135, 1077-9450},
Journal = {JAIDS Journal of Acquired Immune Deficiency Syndromes},
Keywords = {men who have sex with men, female sex workers, HIV, treatment, size estimation, AIDS, Drug Therapy, Human Sex Differences, Same Sex Intercourse, Sex Work, Human Females},
