@article{2012-02378-00320120113,
Abstract = {Comments on an article by D. F. Halpern et al. (see record [rid]2011-23318-001[/rid]). The authors claim that there is no evidence to support positive effects of single-sex education. They acknowledge that 'without blind assessment, randomized assignment to treatment or control experiences, and consideration of selection factors, judging the effectiveness of innovations is impossible,' yet they cite only those studies that support their thesis. Halpern et al. seemed to question whether males and females respond differently to stress, despite abundant research that has been done in this area. They also claim that 'boys who spend more time with other boys become increasingly aggressive' and 'girls who spend more time with other girls become more sex-typed,' yet the source they cite as support studied children whose mean age was 52 months. It is interesting that the authors argue that there is no study using blind assessment and randomized assignments to support the idea that single-sex education leads to stronger outcomes. This is similar to the arguments that the tobacco industry used for years to argue that there was no causal (experimental) evidence to support the notion that cigarette smoking was harmful to one's health. (PsycINFO Database Record (c) 2018 APA, all rights reserved)},
Author = {Kalkus, Olen Anthony},
ISSN = {0036-8075, 1095-9203},
Journal = {Science},
Keywords = {single-sex education, gender stereotyping, sexism, Brain, Education, Female, Humans, Learning, Male, Sex Characteristics, Same Sex Education, Schools, Sexism, Stereotyped Attitudes},
Number = {6065},
Pages = {165 - 165},
Title = {Single-sex education: Results one-sided},
URL = {okalkus@princetonacademy.org},
Volume = {335},
Year = {2012},
}


@article{1999-00463-00519981201,
Abstract = {Explored the effects of parents' and peers' behavior in conversations concerning adolescents' school achievement. A total of about 60 male and female participants were 11.5–14.9 yrs old at 1st measurement of a longitudinal study; there were 2 other assessments at 1-yr intervals. The behavior of adolescents' partners (mothers, fathers, same-sex best friends) was audio-recorded during interactions in a plan-something-together task at 1st measurement. Grades in school and perceived school success were assessed at measurement points 1, 2, and 3. Overall findings suggest that parents and friends have an impact on school performance, but the number of significant associations was small. Parental control behavior had a negative effect on school achievement. There was also a negative effect on perceived school success of parental control actions, while supportive behaviors resulted in improvement of perceived academic performance. While short-term effects of parental challenges seemed to be more negative, the adolescents benefitted from parental challenges in the long run. Control by friends resulted in improvement in adolescents' perceived school performance. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
