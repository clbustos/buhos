Title = {Unseen and unheard: Predictors of sexual risk behavior and HIV infection among men who have sex with men in Chennai, India},
URL = {ssafren@partners.org},
Volume = {21},
Year = {2009},
}


@article{1981-31366-00119810601,
Abstract = {In view of the current interest in children's peer relationships and social skills and the need for valid assessment procedures for children's peer problems, the present study examined the correspondence between peer ratings of acceptance (PRAs) and teacher ratings of a child's social behavior and likability. Ss were 48 males and 44 females from the 3rd, 4th, and 5th grades. Classroom teachers rated each child on withdrawn, aggressive, and likable behaviors using the Pupil Evaluation Inventory. Peer ratings of the child's acceptance in play and work situations were obtained from same-sex classmates. Teacher ratings of likability were the best predictors of peer acceptance scores for males; withdrawn behavior was the best predictor of peer acceptance scores for females. Ratings of withdrawn and aggressive behavior contributed to the prediction of peer acceptance scores for males; for females, only withdrawn behavior contributed to the prediction of peer acceptance. In terms of the clinical utility of teacher ratings for assessing children with peer problems, the use of the withdrawn and/or aggressive ratings scales appeared to offer a promise as a means of identifying such children. (17 ref) (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
