Title = {'War, Guns and Cool, Tough Things': interrogating single-sex classes as a strategy for engaging boys in English},
Volume = {32},
URL = {http://search.ebscohost.com/login.aspx?direct=true&AuthType=ip&db=psyh&AN=2002-11031-002&site=ehost-live},
Year = {2002},
