@article{1996-00439-00919951001,
Abstract = {A study of 55 women and 61 men (aged 18–68 yrs) examined the relationships among S's sex, experimenter sex, sex roles, and sexual attitudes as predictors of drawing the same sex figure on the Draw A Person test. Results indicate that S and experimenter sex are consistent and significant predictors of picture sex. Most Ss drew a same-sex figure first as opposed to an opposite-sex figure. Results also show that male Ss were more likely to draw an opposite-sex female figure than female Ss were to draw an opposite-sex male figure. Masculinity and sexual attitudes also predicted figure sex, but only for female picture attributes. The authors suggest that Ss' response to current, historically determined attitudes toward gender and gender roles must be seen as strong influences on the sex of the drawn picture. A model of some determinants of the sex of the drawn figure is offered. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
Author = {Houston, Ann Nikolai and  Terwilliger, Robert},
