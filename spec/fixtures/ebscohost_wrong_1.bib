@article{1982-06372-00119810601,
Abstract = {To determine the relationship of sex and academic performance to recommendations to graduate school, 410 letters of recommendation were examined (233 letters written for men, 177 for women) by 2 raters who were blind to the sex of the applicants and referees. Letters were rated for 11 personal qualities and for indications of weaknesses. GPA was found to be related to ratings of intellectual potential, academic performance, research ability, and motivation. The main effect of sex of applicant was not significant for any variable; however, referees tended to describe same-sex applicants as having more motivation and fewer weaknesses than opposite-sex applicants. (9 ref) (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
