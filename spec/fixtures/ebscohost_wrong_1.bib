@article{2013-34753-00420131001,
Abstract = {Reports an error in 'Single-sex public education for low-income youth of color: A critical theoretical review' by Sara Goodkind (Sex Roles, Advanced Online Publication, Oct 9, 2012, np). The original article erroneously states that it was 2006 changes to Title IX that eased legal impediments to single-sex public education in the United States. Title IX was not amended. Instead, the U.S. Department of Education issued new regulations concerning single-sex schooling in 2006. (The following abstract of the original article appeared in record [rid]2013-33195-001[/rid]). There has been a widespread increase in single-sex public schooling in the U.S. following 2006 amendments to Title IX stipulated by the No Child Left Behind Act. As its inclusion in No Child Left Behind demonstrates, single-sex public schooling is viewed as a means to improve the educational experiences and performance of low-income youth of color. Yet little is known about the effects and efficacy of single-sex public education, particularly for these populations. This piece provides a critical theoretical review of this issue. It examines the three main rationales offered by proponents of single-sex education for low-income youth of color, who contend that it will 1) eliminate distraction by and harassment from the other sex; 2) address the different learning styles of girls and boys; and 3) remedy past inequities experienced by low-income populations of color by offering them opportunities afforded to more privileged youth. The review reveals no documented benefits to single-sex public education for low-income youth of color and a number of concerns. Thus, it recommends that policy makers reconsider devoting scarce resources to this practice, which ultimately has the potential to further marginalize low-income youth of color by reinforcing racialized stereotypes of hypersexuality, reproducing narrow and restrictive definitions of gender, invalidating the experiences and identities of GLBTQ youth, and diverting attention from the need to address poverty and racism. (PsycINFO Database Record (c) 2019 APA, all rights reserved)},
Author = {Goodkind, Sara},
ISSN = {0360-0025, 1573-2762},
Journal = {Sex Roles: A Journal of Research},
Keywords = {single sex education, public education, low-income youth, learning style, race, Lower Income Level, Public School Education, Race (Anthropological), Same Sex Education, Cognitive Style},
Number = {7-8},
Pages = {403 - 403},
Title = {Erratum to: 'Single-sex public education for low-income youth of color: A critical theoretical review'},
URL = {sag51@pitt.edu},
Volume = {69},
Year = {2013},
}


@article{1973-07854-00119720801,
Abstract = {Conducted a study with a total of 1,225 New Zealand secondary school students in 2 single-sex and 1 coeducational schools. Ss were given a questionnaire assessing attitudes, beliefs, and behaviors in the areas of scholarship, prestige, popularity, peer influences on behavior, and personal goals. Schools were similar in curricula, student regimentation, and attitudes and values of teachers, administrators, and Ss' parents. Significant differences were found between Ss in the single-sex schools and Ss of the same sex in the coeducational school in all of the above areas. Results suggest coeducation may be inimical to both academic achievement and social adjustment. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
