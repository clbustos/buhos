@article{2001-14480-00820000201,
Abstract = {A sense of academic competence combines at least 2 forms of gender stereotyping: an illusory glow about performance on specific tasks, and traditional gender stereotyping about general perceptions of natural talent. Flexible categorization in terms of generality and content suggests a multifaceted model of Aspects of Self-Knowledge about Mathematics and English (ASK-ME). This paper demonstrates the flexibility of the ASK-ME model for adolescents (N = 1,360; aged 11–15 yrs) in 2 social contexts (coed and single-gender schools). The forms of gender stereotyping combine so that where traditional gender stereotyping was reduced at single-gender schools, an illusory glow had more influence. One outcome was that boys in single-gender settings expressed enhanced perceptions of performance in mathematics and language. Results highlight the importance of generality and content in understanding the gendered nature of academic self-concepts in social contexts. Implications are for differential influences on the plans and choices adolescents make about work and study. (PsycINFO Database Record (c) 2019 APA, all rights reserved)},
Author = {Bornholt, Laurel J.},
ISSN = {0021-9029, 1559-1816},
Journal = {Journal of Applied Social Psychology},
Keywords = {illusion about achievement & traditional gender stereotype about natural talent as components of academic competence measured by ASK-ME model, 11–15 yr old males vs females in coed vs same-sex classes, Academic Achievement, Academic Self Concept, Competence, Human Sex Differences, Stereotyped Attitudes, Academic Aptitude, Academic Environment, High School Students, Middle School Students, Models, Single Sex Environments, Self-Knowledge},
