@article{ WOS:A1992JH26100004,
Author = {LEE, VE and MARKS, HM},
Title = {{WHO GOES WHERE - CHOICE OF SINGLE-SEX AND COEDUCATIONAL INDEPENDENT
   SECONDARY-SCHOOLS}},
Journal = {{SOCIOLOGY OF EDUCATION}},
Year = {{1992}},
Volume = {{65}},
Number = {{3}},
Pages = {{226-253}},
Month = {{JUL}},
Abstract = {{Whether families and students choose single-sex over coeducational
   schools for traditional reasons or because they value these schools as
   opportunity structures is the focus of this study. In the United States,
   where single-sex education is an option only in the private sector, most
   research on this organizational form has focused on Catholic schools.
   Introducing a new population to single-sex school research, this sample
   consists of the entire 1989 senior class (3,183 students) in each of 60
   secondary schools selected as a stratified random sample of girls',
   boys', and coeducational independent schools. The study considers the
   personal, demographic, and educational characteristics of families and
   students, as well as their reasons for selecting the particular school
   they attend. Because boys and girls may have different reasons for
   choosing schools and because previous research on this topic documented
   differential gender effects, the study examines the question separately
   by gender. A family tradition for private schooling (mostly single-sex)
   and a religious orientation characterize students who select the
   single-sex school option.}},
