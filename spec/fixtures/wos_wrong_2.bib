@article{ WOS:A1992JN28800008,
Author = {MONACO, NM and GAIER, EL},
Title = {{SINGLE-SEX VERSUS COEDUCATIONAL ENVIRONMENT AND ACHIEVEMENT IN
   ADOLESCENT FEMALES}},
Journal = {{ADOLESCENCE}},
Year = {{1992}},
Volume = {{27}},
Number = {{107}},
Pages = {{578-594}},
Month = {{FAL}},
Abstract = {{For women, the nature and range of experiences during the high school
   years take on special significance, since it is during this period that
   they usually weigh their various roles and adjust their levels of
   aspirations accordingly. If the high school environment is successful in
   reducing the discrepancy between what are often viewed as conflicting
   roles, adolescent females may place greater emphasis on achievement. It
   is within this context that the present paper explored the differential
   benefits of single-sex and coeducational schooling. The issue explored
   is not whether one is preferable for females; rather, the concern here
   is how each of these settings influences both achievement and personal
   fulfillment.}},
