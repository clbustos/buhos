@article{ WOS:000210261600004,
Author = {Kissau, Scott and Quach, Lan and Wang, Chuang},
Title = {{Impact of Single-sex Instruction on Student Motivation to Learn Spanish}},
Journal = {{CANADIAN JOURNAL OF APPLIED LINGUISTICS}},
Year = {{2009}},
Volume = {{12}},
Number = {{2}},
Pages = {{54-78}},
Abstract = {{To increase male motivation to learn additional languages studies have
   suggested teaching males in single-sex second and foreign language
   classes (Carr \& Pauwels, 2006; Chambers, 2005). Despite the reported
   benefits of this unique arrangement, a review of literature found no
   related research conducted in Canada or the United States. To address
   this lack of research, a study was conducted in the spring of 2008 to
   investigate the impact of single-sex instruction on student motivation
   to learn Spanish. Using Gardner's model of second language motivation
   (1985), 57 high-school students studying Spanish in either single-sex or
   coeducational classes completed a pre and post questionnaire to gauge
   their motivation to learn the language. Follow-up interviews were also
   conducted with both students and teachers. Results indicated that while
   both sexes enjoyed some educational advantages from the single-sex
   environment, the benefits appeared to be greater for the males than the
   females.}},
