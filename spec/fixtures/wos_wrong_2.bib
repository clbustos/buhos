@article{ WOS:000473837600001,
Author = {Choi, Eleanor Jawon and Moon, Hyungsik Roger and Ridder, Geert},
Title = {{Within-District School Lotteries, District Selection, and the Average
   Partial Effects of School Inputs}},
Journal = {{KOREAN ECONOMIC REVIEW}},
Year = {{2019}},
Volume = {{35}},
Number = {{2}},
Pages = {{275-306}},
Month = {{SUM}},
Abstract = {{This study proposes an econometric framework to consistently estimate
   the average partial effects (APE) of school inputs on academic
   achievement when students are randomly assigned to schools within each
   school district but endogenously sort across school districts. We
   illustrate our method by estimating the APE of single-sex schooling and
   class size on standardized test scores using data from Seoul, Korea in
   the period of 2008-2009. Our APE estimates are smaller than the
   estimates from conventional linear regressions with school district
   fixed effects, which do not fully correct for endogenous district choice
   and thus suffer from selection bias.}},
