@inproceedings{ WOS:000326396405111,
Author = {Hyun, Junghwan and Kwak, Dowon},
Editor = {{Chova, LG and Martinez, AL and Torres, IC}},
Title = {{THE EFFECT OF SINGLE-SEX SCHOOLS ON ACADEMIC ACHIEVEMENT: USING NATURAL
   EXPERIMENT DATA OF KOREAN HIGH SCHOOL}},
Booktitle = {{INTED2012: INTERNATIONAL TECHNOLOGY, EDUCATION AND DEVELOPMENT
   CONFERENCE}},
Series = {{INTED Proceedings}},
Year = {{2012}},
Pages = {{5794-5798}},
Note = {{6th International Conference of Technology, Education and Development
   (INTED), Valencia, SPAIN, MAR 05-07, 2012}},
Abstract = {{We study the effect of single-sex school on high school student academic
   performance using 10 yearlong panel data on 202 randomized high schools
   in Seoul. Our sample includes all randomized schools during 2000-2010 in
   Seoul. Out of 202 randomized high schools, there are 59 female schools,
   69 male schools, and 74 coeducational schools in 2010. These high
   schools are randomized in the sense that students are randomly assigned
   to one of two types of schools within a school district: single-sex and
   coeducational. We find the evidence that (i) the average scores for
   single-sex schools are higher than those of coeducational schools for
   both all-boys and all-girls schools, (ii) the average effect is stronger
   for all-girl schools, and quantile regression results report that (iii)
   the positive effects of single-sex school are much more significant for
   underachieving single-sex schools, and (iv) the effect becomes
   statistically insignificant for all-girls schools in 60 percentile and
   above while the effects for all-boys schools are significant for all
   quantiles of the score distribution.}},
